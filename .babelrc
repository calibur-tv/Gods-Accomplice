{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 3 versions",
            "iOS > 9",
            "chrome >= 33"
          ]
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "babel-plugin-lodash",
    ["component", { "libraryName": "element-ui", "styleLibraryName": "theme-chalk"}, "element-ui"],
    ["component", { "libraryName": "mint-ui", "style": false}, "mint-ui"],
    [
      "import",
      {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}
